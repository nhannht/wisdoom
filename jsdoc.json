{
  "tags": {
      "allowUnknownTags": true
  },
  "source": {
      "include": ["./src"]
  },
  "plugins": [
      "plugins/markdown",
      "better-docs/component"
  ],
  "opts": {
      "encoding": "utf8",
      "destination": "docs/",
      "recurse": true,
      "template": "./node_modules/better-docs"
  }
}