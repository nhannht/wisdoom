{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "./src"
    ]
  },
  "plugins": [
    "plugins/markdown",
    "better-docs/component",
    "better-docs/category"
  ],
  "opts": {
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "template": "./node_modules/better-docs",
    "readme": "./README.md"
  },
  "templates": {
    "search": true,
    "cleverLinks": true,
    "better-docs": {
      "navLinks": [
        {
          "label": "Github",
          "href": "https://github.com/nhannht/wisdoom"
        }
      ],
      "footer": "hello footer"

    }
  }
}