{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "./src"
    ]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "template": "./node_modules/tui-jsdoc-template",
    "readme": "./README.md"
  },
  "templates": {
    "search": true,
    "cleverLinks": true,
    "logo": {
      "url":"https://raw.githubusercontent.com/nhannht/wisdoom/master/src/icons/wisdoomIcon.svg",
      "width": "30px",
      "height": "30px",
      "link": "https://nhannht.github.io/wisdoom/"
    },
    "name": "Wisdoom",
    "footerText": "Let the wisdom of the crowd guide you"
  }
}