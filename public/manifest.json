{
  "short_name": "Wisdoom",
  "name": "Wisdoom",
  "description": "Inject knowledge everywhere",
  "author": "nhannht",
  "manifest_version": 3,
  "version": "0.1.0",
  "icons": {
    "16": "icons/appicon-style-larva-wisdom-computation-meditation-flat-icon-33881868.png",
    "48": "icons/appicon-style-larva-wisdom-computation-meditation-flat-icon-33881868.png",
    "128": "icons/appicon-style-larva-wisdom-computation-meditation-flat-icon-33881868.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./static/js/content.js"
      ],
      "css": [
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "storage",
    "notifications",
    "webNavigation",
    "downloads",
    "clipboardWrite"
  ]
}
