{
  "short_name": "Wisdoom",
  "name": "Wisdoom",
  "description": "Inject knowledge everywhere",
  "author": "nhannht",
  "manifest_version": 3,
  "version": "0.1.2",
  "icons": {
    "16": "icons/larva-meditation128.png",
    "48": "icons/larva-meditation128.png",
    "128": "icons/larva-meditation128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./static/js/content.js",
        "myjs.js"
      ],
      "css": [
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "downloads",
    "clipboardWrite"
  ]
}
